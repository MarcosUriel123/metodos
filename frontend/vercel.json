{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/pages/**/*.js", 
      "use": "@vercel/static"
    },
    {
      "src": "src/pages/**/*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/pages/index/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/pages/index/index.html"
    }
  ]
}